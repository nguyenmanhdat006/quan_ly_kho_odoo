<odoo>
    <menuitem id="menu_root" name="School"/>

    <menuitem id="menu_student" name="Students" parent="menu_root" sequence="1"/>
    <menuitem id="menu_certificate" name="Certificates" parent="menu_root" sequence="2"/>

    <record id="action_student" model="ir.actions.act_window">
        <field name="name">Students</field>
        <field name="res_model">student</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_certificate" model="ir.actions.act_window">
        <field name="name">Certificates</field>
        <field name="res_model">certificate</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_student_action" parent="menu_school_student" action="action_student"/>
    <menuitem id="menu_certificate_action" parent="menu_school_certificate" action="action_certificate"/>
</odoo>
