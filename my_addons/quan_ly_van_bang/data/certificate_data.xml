<odoo>
    <!-- các tổ chức cấp chứng chỉ -->
    <record id="issuing_organization_idp" model="issuing.organization">
        <field name="name">IDP</field>
    </record>

    <record id="issuing_organization_bc" model="issuing.organization">
        <field name="name">British Council</field>
    </record>

    <record id="issuing_organization_llg_vn" model="issuing.organization">
        <field name="name">LLG VN</field>
    </record>

    <!-- các loại chứng chỉ -->
    <record id="certificate_type_ielts" model="certificate.type">
        <field name="name">IELTS</field>
        <field name="issuing_organization_ids" eval="[(6, 0, [ref('issuing_organization_idp'), ref('issuing_organization_bc')])]"/>
    </record>

    <record id="certificate_type_toeic" model="certificate.type">
        <field name="name">TOEIC</field>
        <field name="issuing_organization_ids" eval="[(6, 0, [ref('issuing_organization_llg_vn')])]"/>
    </record>
</odoo>